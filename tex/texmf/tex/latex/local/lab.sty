\newcounter{SemesterWeek}
\newcommand{\duedatepreproject}[8]{%
  \textbf{Assigned:} \SemesterDate{#1}{#2} \\ %
  \textbf{Pre-project write-up due:} #5, \SemesterDate{#3}{#4} \\%
  \textbf{Due:} #8, \SemesterDate{#6}{#7} %
  \setcounter{SemesterWeek}{#2}
}
\newcommand{\duedate}[5]{%
  \textbf{Assigned:} \SemesterDate{#1}{#2} \\ %
  \textbf{Due:} #5, \SemesterDate{#3}{#4} %
  \setcounter{SemesterWeek}{#2}
}
\newcommand{\doubleduedate}[9]{%
  \textbf{Assigned:} \SemesterDate{#1}{#2} \\ %
  \textbf{Due:} #5, \SemesterDate{#3}{#4} #6 \\ %
  \textbf{Due:} #9, \SemesterDate{#7}{#8} %
}

